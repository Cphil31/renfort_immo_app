{% extends 'base.html.twig' %}

{% block title %}Produit{% endblock %}

{% block body %}
    <h1>Produit</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ produit.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ produit.name }}</td>
            </tr>
            <tr>
                <th>Localisation</th>
                <td>{{ produit.localisation }}</td>
            </tr>
            <tr>
                <th>CarTechniques</th>
                <td>{{ produit.carTechniques }}</td>
            </tr>
            <tr>
                <th>CarJuridiques</th>
                <td>{{ produit.carJuridiques }}</td>
            </tr>
            <tr>
                <th>CarCommerciales</th>
                <td>{{ produit.carCommerciales }}</td>
            </tr>
            <tr>
                <th>Environement</th>
                <td>{{ produit.environement }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_produit_index') }}">back to list</a>

    <a href="{{ path('app_produit_edit', {'id': produit.id}) }}">edit</a>

    {{ include('produit/_delete_form.html.twig') }}
