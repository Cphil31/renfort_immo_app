{% extends 'base.html.twig' %}

{% block title %}StatutEntreprise index{% endblock %}

{% block body %}
    <h1>StatutEntreprise index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Statut</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for statut_entreprise in statut_entreprises %}
            <tr>
                <td>{{ statut_entreprise.id }}</td>
                <td>{{ statut_entreprise.statut }}</td>
                <td>
                    <a href="{{ path('app_statut_entreprise_show', {'id': statut_entreprise.id}) }}">show</a>
                    <a href="{{ path('app_statut_entreprise_edit', {'id': statut_entreprise.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_statut_entreprise_new') }}">Create new</a>
{% endblock %}
