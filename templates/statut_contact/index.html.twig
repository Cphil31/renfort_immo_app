{% extends 'base.html.twig' %}

{% block title %}StatutContact index{% endblock %}

{% block body %}
    <h1>StatutContact index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Statut</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for statut_contact in statut_contacts %}
            <tr>
                <td>{{ statut_contact.id }}</td>
                <td>{{ statut_contact.statut }}</td>
                <td>
                    <a href="{{ path('app_statut_contact_show', {'id': statut_contact.id}) }}">show</a>
                    <a href="{{ path('app_statut_contact_edit', {'id': statut_contact.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_statut_contact_new') }}">Create new</a>
{% endblock %}
