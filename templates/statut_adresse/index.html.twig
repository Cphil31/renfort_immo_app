{% extends 'base.html.twig' %}

{% block title %}StatutAdresse index{% endblock %}

{% block body %}
    <h1>StatutAdresse index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Statut</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for statut_adresse in statut_adresses %}
            <tr>
                <td>{{ statut_adresse.id }}</td>
                <td>{{ statut_adresse.statut }}</td>
                <td>
                    <a href="{{ path('app_statut_adresse_show', {'id': statut_adresse.id}) }}">show</a>
                    <a href="{{ path('app_statut_adresse_edit', {'id': statut_adresse.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_statut_adresse_new') }}">Create new</a>
{% endblock %}
